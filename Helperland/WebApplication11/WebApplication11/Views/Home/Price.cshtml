<body>
    <!--header code start-->
    <header id="price_section">
        <nav class="navbar navbar-expand-lg navbar-fixed-top ">
            <div class="container-fluid ">
                <a asp-action="Index" class="navbar-brand">
                    <img src="~/IMAGES/logo.png" height="28" alt="CoolBrand" />
                </a>
                <button class="navbar-toggler ddmenu" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse"
                        aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                    <img src="~/IMAGES/list.svg" alt="List" width="50px" height="50px">

                </button>
                <div class="collapse navbar-collapse justify-content-end" id="navbarCollapse">
                    <div class="navbar-nav text-light">
                        <a asp-action="Book_service" class="nav-item nav-link menu menu1">Book Now</a>
                        <a asp-action="Price" class="nav-item nav-link menu">Prices & Services</a>
                        <a href="#" class="nav-item nav-link menu">Warranty</a>
                        <a href="#" class="nav-item nav-link menu">Blog</a>
                        <a asp-action="Contact" class="nav-item nav-link menu">Contact</a>
                        <button class="nvbarbtn">Login</button>
                        <a asp-action="provider" id="nvbarbtn" class="nvbarbtn">Become a Helper </a>

                        @*<button asp-action="provider" id="nvbarbtn" class="nvbarbtn">Become a Helper </button>*@
                    </div>
                </div>
            </div>
        </nav>
        <div>
            <img src="~/IMAGES/bgimage.png" class="img-fluid bg" />
        </div>
    </header>
    <!--header code end-->
    <!--price heading section start-->
    <section class="price-heading">
        <div class="heading">
            <h1 class="text-center">Price</h1>
        </div>
        <div class="d-flex star justify-content-center">
            <img src="~/IMAGES/star.png" alt="" />
        </div>
        <div class="box">
            <div class="card">
                <div class="text-center b-text">
                    One Time
                </div>
                <div class="d-flex justify-content-center align-items-center b-value">
                    &euro; 20 <sub>/hr</sub>

                </div>
                <div class="row g-2 caption">
                    <div class="col-sm-8 col-md-7 col-11 icon">
                        <img src="~/IMAGES/tickmark.svg" alt="" />Lower prices
                    </div>
                    <div class="col-sm-8 col-md-7 col-11 icon">
                        <img src="~/IMAGES/tickmark.svg" alt="" />Easy online & secure payment
                    </div>
                    <div class="col-sm-8 col-md-7 col-11 icon">
                        <img src="~/IMAGES/tickmark.svg" alt="" />Easy amendment
                    </div>
                </div>
            </div>

        </div>
        <div class="d-flex justify-content-center align-items-center">
            <hr / class="line">
        </div>
    </section>
    <!--price heading section end-->
    <!--extra services code start-->
    <section class="extra-service">
        <div class="eservice-heading">
            <h1 class="text-center">Extra Services</h1>
        </div>
        <div class="d-flex star justify-content-center">
            <img src="~/IMAGES/star.png" alt="" />
        </div>
        <div class="d-flex align-items-center justify-content-between flex-wrap service-icons">
            <div>
                <div class="rounded-circle d-flex justify-content-center align-items-center s-icon">
                    <img src="~/IMAGES/3.png" alt="3" />
                </div>
                <div class="text-center image-heading">
                    Incide Cabinets
                </div>
                <div class="text-center image-time">
                    30minutes
                </div>
            </div>

            <div>
                <div class="rounded-circle d-flex justify-content-center align-items-center s-icon">
                    <img src="~/IMAGES/5.png" alt="5" />
                </div>
                <div class="text-center image-heading">
                    Incide Fridge
                </div>
                <div class="text-center image-time">
                    30minutes
                </div>
            </div>

            <div>
                <div class="rounded-circle d-flex justify-content-center align-items-center s-icon">
                    <img src="~/IMAGES/4.png" alt="4" />
                </div>
                <div class="text-center image-heading">
                    Inside Oven
                </div>
                <div class="text-center image-time">
                    30minutes
                </div>
            </div>

            <div>
