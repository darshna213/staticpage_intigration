        <div class="row container-fluid cleaning-card">
            <div class="card col-md-3 col-sm-4 col-lg-4">
                <img src="~/IMAGES/card-1.png" class="card-img-top" />
                <div class="card-body">
                    <h5 class="card-title">Bedroom and Living Room</h5>
                    <div class="card-line">
                        <img src="~/IMAGES/right-arrow.png" alt="" />
