                    <img src="~/IMAGES/2.png" alt="2" />
                </div>
                <div class="text-center image-heading">
                    Laundry wash & dry
                </div>
                <div class="text-center image-time">
                    30minutes
                </div>
            </div>

            <div>
                <div class="rounded-circle d-flex justify-content-center align-items-center s-icon">
                    <img src="~/IMAGES/1.png" alt="1" />
                </div>
                <div class="text-center image-heading">
                    Interior windows
                </div>
                <div class="text-center image-time">
                    30minutes
                </div>

            </div>

        </div>
    </section>
    <!--extra services code end-->
    <!--card code start-->
    <section class="cleaning container-fluid" id="cleaning-heading">
        <div class="cleaning-heading">
            <h1 class="text-center">What we include in cleaning</h1>
        </div>
        <div class="d-flex star justify-content-center">
            <img src="~/IMAGES/star.png" alt="" />
